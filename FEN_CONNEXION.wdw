     procedures :
      -
        name : ConnecteClientServeur
        procedure_id : 2039461990691262196
        type_code : 14
        code : |1+
         Procédure ConnecteClientServeur()
         
         // Paramètres de la connexion
         gcnxHFSQLCS..Provider = HAccèsHFClientServeur
         gcnxHFSQLCS..Utilisateur = SAI_UTILISATEUR
         gcnxHFSQLCS..MotDePasse = SAI_MOT_DE_PASSE
         gcnxHFSQLCS..Serveur = SAI_SERVEUR + ":" + SAI_NUMERO_PORT
         gcnxHFSQLCS..BaseDeDonnées = SAI_Base_de_données
         gcnxHFSQLCS..Cryptage = hCryptageNon
         
         // Ouverture de la connexion
         si HOuvreConnexion(gcnxHFSQLCS) = faux ALORS
         	erreur("Impossible de se connecter au serveur.","Vérifier les paramètres saisis.")
         	reprisesaisie()
         FIN
         
         // Si la base de données est vide, on ne fait pas le changement de connexion
         si pas SAI_Base_de_données ~= "" ALORS
         	// Changement de la connexion (pour tous les fichiers de l'analyse du projet)
         	HChangeConnexion("*",gcnxHFSQLCS)
         FIN
         
        type : 458752
