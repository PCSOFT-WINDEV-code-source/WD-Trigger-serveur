           code : |1-
            
            // Vérification des champs
            
            // Vérification du serveur
            si SAI_SERVEUR ~= "" ALORS
            	erreur(<§$0000§>)
            	reprisesaisie(SAI_SERVEUR)
            FIN
            
            // Vérification du nom d'utilisateur
            Si SAI_NUMERO_PORT = 0 ALORS
            	erreur(<§$0001§>)
            	reprisesaisie(SAI_NUMERO_PORT)
            FIN
            
            // Vérification du nom d'utilisateur
            Si SAI_UTILISATEUR ~= "" ALORS
            	erreur(<§$0002§>)
            	reprisesaisie(SAI_UTILISATEUR)
            FIN
            
            // Tout est bon, on tente de se connecter
            ConnecteClientServeur()
            
            mafenêtre..valeurrenvoyée = vrai
            
            ferme()
           type : 18
         -
